<%- include header %>
  
  <div class="row">
    
    <div class="col-md-12">
      
      <% if (events.length === 0) { %>
        <h2>Nu sunt evenimente pentru aceasta perioada.</h2>
      <% } %>

      <% events.forEach(function(event, i) { %>
          
        <div class="row event">
          
          <%- include event-front %>
          
        </div>
        
      <% }) %>

    </div>

  </div>
  
<%- include footer %>