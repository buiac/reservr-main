<%- include header %>
  
  <div class="row">
    
    <div class="col-md-12">

      <% if (events.length === 0) { %>

        <h3 class="text-center">There are no events.</h3>
        
      <% } else { %>
        
        <% events.forEach(function(event, i) { %>
            
          <div class="row event">
            
            <%- include event-front %>
            
          </div>
          
        <% }) %>

      <% } %>

    </div>

  </div>
  
<%- include footer %>