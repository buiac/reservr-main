<% var theEvent = '' %>
<%- include header %>
    
    

      <h2>Reservations</h2>

      <h4><a href="/dashboard/<%= org._id %>/event/<%= event._id %>"><%= event.name %></a>, <span style="font-size:14px"><%= moment(event.date).format() %></span></h4>
      

      <table class="table-reservations table table-striped">
        <thead>
          <tr>
            <th>
              Name
            </th>
            <th>
              Date &amp; time
            </th>
            <th>
              Email
            </th>
            <th>
              Seats
            </th>
            <th></th>
          </tr>
        </thead>
        <tbody>

            <tr>
              <td>

                <%= reservation.name %>

              </td>
              <td>
                <% if (reservation.timestamp) { %>
                  <%= moment(reservation.timestamp).format() %>
                <% } %>
              </td>
              <td>
                <%= reservation.email %>
              </td>
              <td>
                <%= reservation.seats %>
              </td>
              <td>
                <a href="/r/delete-reservation/<%= reservation._id %>" class="btn-delete-reservation">
                  <i class="fa fa-times-circle"></i>
                  Delete Reservation
                </a>
              </td>
            </tr>

        </tbody>
        
      </table>
      


<%- include footer %>
